{
  "name": "copona/core",
  "type": "library",
  "description": "Copona Core",
  "bin": ["bin/copona"],
  "config": {
    "bin-dir": "./"
  },
  "keywords": [
    "copona",
    "ecommerce",
    "framework",
    "opensource",
    "core"
  ],
  "homepage": "https://github.com/copona/copona",
  "license": "MIT",
  "require": {
    "php": ">=5.6.0",
    "cardinity/cardinity-sdk-php": "^1.0",
    "braintree/braintree_php": "3.2.0",
    "leafo/scssphp": "0.0.12",
    "divido/divido-php": ">=1.1.1",
    "klarna/kco_rest": "^2.2",
    "phpmailer/phpmailer": "^5.2",
    "robmorgan/phinx": "^0.8",
    "filp/whoops": "^2.1",
    "hassankhan/config": "^0.10.0",
    "vlucas/phpdotenv": "^2.4",
    "illuminate/container": "^5.4",
    "twig/twig": "~1.0",
    "symfony/finder": "^3.3",
    "illuminate/support": "^5.4",
    "symfony/console": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Copona\\": "./src",
      "Copona\\System\\": "/system/",
      "Copona\\System\\Library\\": "/system/library/",
      "Copona\\System\\Engine\\": "/system/engine/",
      "Extension\\": "/extensions/",
      "Theme\\": "/themes/"
    }
  }
}